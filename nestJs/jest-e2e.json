{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "src",
  "testEnvironment": "node",
  "testRegex": ".*\\.e2e\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },  
  "coverageDirectory": "../coverage-e2e",
  "collectCoverageFrom": [
    "**/*.(t|j)s",
    "!**/*.spec.ts"
  ]
}
